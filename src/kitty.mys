from base64 import encode

def write_png(data: bytes):
    encoded = encode(data)
    print("\x1b_Ga=T,f=100,m=1\x1b\\", end="")

    for offset in range(0, i64(len(encoded)), 4096):
        print("\x1b_Gm=1;", end="")
        print(encoded[offset:offset + 4096], end="")
        print("\x1b\\", end="")

    print("\x1b_Gm=0\x1b\\\n", end="")
